{
  "name": "electron-react-app",
  "version": "0.1.0",
  "private": true,
  "main": "electron/main.js",
  "homepage": "./",
  "dependencies": {
    "@tanstack/react-query": "^5.51.11",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/react-window": "^1.8.8",
    "axios": "^1.7.7",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "electron-is-dev": "^3.0.1",
    "express": "^5.1.0",
    "lucide-react": "^0.546.0",
    "react": "^19.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.28.0",
    "react-scripts": "5.0.1",
    "react-window": "^2.2.1",
    "recharts": "^2.13.4",
    "web-vitals": "^2.1.4",
    "zod": "^3.23.8",
    "zustand": "^4.5.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "electron:dev": "wait-on tcp:3001 && electron .",
    "electron:build": "electron-builder",
    "start:renderer": "cross-env BROWSER=none PORT=3001 REACT_APP_API_URL=http://127.0.0.1:8001 react-scripts start",
    "start:electron": "concurrently \"npm run start:renderer\" \"npm run electron:dev\"",
    "build:electron": "npm run build && electron-builder",
    "lint": "eslint ./src --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss}\"",
    "preview": "serve -s build",
    "dev": "npm run start:electron",
    "dev:full": "concurrently --kill-others \"npm run start:renderer\" \"wait-on tcp:3001 && electron .\"",
    "postinstall": "electron-builder install-app-deps"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.7.5",
    "@types/react": "^18.3.10",
    "@types/react-dom": "^18.3.5",
    "concurrently": "^9.2.1",
    "cross-env": "^7.0.3",
    "electron": "^38.2.2",
    "electron-builder": "^26.0.12",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^4.6.2",
    "prettier": "^3.3.3",
    "serve": "^14.2.3",
    "typescript": "^4.9.5",
    "wait-on": "^9.0.1"
  },
  "build": {
    "appId": "com.example.electron-react-app",
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
